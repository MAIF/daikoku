    "start": "webpack-dev-server --port 3000 --host 0.0.0.0 --live-reload=true -d --color",
    "watch": "webpack --watch --mode development --live-reload=true -d --color",
    "build": "rm -rf ../public/react-app && mkdir ../public/react-app && webpack --mode production -p",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
