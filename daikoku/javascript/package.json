{
  "name": "daikoku-ui",
  "version": "1.0.0",
  "description": "UI for daikoku",
  "main": "src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier": "prettier --write --single-quote --trailing-comma es5 --print-width 100 --tab-width 2 --jsx-bracket-same-line \"src/**/*.{ts, tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "start": "webpack serve --port 3000 --host 0.0.0.0 --live-reload --color --mode development",
    "watch": "webpack --watch --mode development --live-reload -d --color",
    "build": "rm -rf ../public/react-app && mkdir ../public/react-app && webpack --mode production --progress",
    "cypress": "cypress open",
    "test:cypress": "cypress run --record --key $CYPRESS_RECORD_KEY",
    "cypress:report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "cypress:report:generate": "marge cypress/results/index.json --reportDir report --assetsDir report/assets --reportPageTitle index.html",
    "cypress:start:backend": "cd .. && sbt  'run -Dhttp.port=9000 -Ddaikoku.storage=postgres -Ddaikoku.postgres.database=daikoku_test -Ddaikoku.postgres.user=postgres -Ddaikoku.init.data.from=javascript/cypress/cypress-test-export.ndjson'",
    "cypress:start:backend-dev": "cd .. && sbt  'run -Dhttp.port=9000 -Ddaikoku.mode=Dev -Ddaikoku.storage=postgres -Ddaikoku.postgres.database=daikoku_test -Ddaikoku.postgres.user=postgres -Ddaikoku.init.data.from=javascript/cypress/cypress-test-export.ndjson'"
  },
