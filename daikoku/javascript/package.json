{
  "name": "daikoku-ui",
  "version": "1.0.0",
  "description": "UI for daikoku",
  "main": "src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prettier": "prettier --write --single-quote --trailing-comma es5 --print-width 100 --tab-width 2 --jsx-bracket-same-line \"src/**/*.js\"",
    "test:eslint": "eslint src/ --ext .js --ext .jsx",
    "start": "webpack serve --port 3000 --host 0.0.0.0 --liveReload --color --mode development",
    "watch": "webpack --watch --mode development --liveReload -d --color",
    "build": "rm -rf ../public/react-app && mkdir ../public/react-app && webpack --mode production --progress",
    "cypress": "cypress open",
    "test:cypress": "cypress run --record --key $CYPRESS_RECORD_KEY",
    "cypress:report:merge": "mochawesome-merge cypress/results/json/*.json > cypress/results/index.json",
    "cypress:report:generate": "marge cypress/results/index.json --reportDir report --assetsDir report/assets --reportPageTitle index.html",
    "cypress:start:backend": "cd .. && sbt  'run -Dhttp.port=9000 -Ddaikoku.init.data.from=javascript/cypress/cypress-test-export.ndjson'"
  },
  "dependencies": {
    "@apollo/client": "^3.4.8",
    "@fortawesome/fontawesome-free": "5.15.4",
    "@maif/react-forms": "1.0.1",
    "@xstate/react": "1.5.1",
    "antd": "4.16.12",
    "asciidoctor": "2.2.5",
    "bcryptjs": "2.4.3",
    "bootstrap": "4.5.3",
    "byte-converter": "0.1.4",
    "classnames": "2.3.1",
    "connected-react-router": "6.9.1",
    "es6-shim": "0.35.6",
    "faker": "5.5.3",
    "get-value": "3.0.1",
    "graphql": "^15.5.1",
    "highlight.js": "10.5.0",
    "jquery": "3.6.0",
    "js-md5": "0.7.3",
    "json5": "2.2.0",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "popper.js": "1.16.1",
    "prop-types": "15.7.2",
    "query-string": "6.13.8",
    "react": "17.0.2",
    "react-ace": "9.4.3",
    "react-color": "2.19.3",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-modal": "3.14.3",
    "react-paginate": "7.1.3",
    "react-redux": "7.2.4",
    "react-redux-toastr": "7.6.5",
    "react-router-dom": "5.2.0",
    "react-select": "3.1.1",
    "react-step-wizard": "5.3.9",
    "react-table": "7.7.0",
    "react-tooltip": "4.2.21",
    "recharts": "1.8.5",
    "redux": "4.1.1",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "set-value": "3.0.2",
    "showdown": "1.9.1",
    "swagger-editor": "3.18.0",
    "swagger-ui-dist": "3.38.0",
    "uuid": "8.3.2",
    "whatwg-fetch": "3.6.2",
    "xstate": "4.23.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-minify-webpack-plugin": "0.3.1",
    "compression-webpack-plugin": "6.1.1",
    "crypto-browserify": "^3.12.0",
    "css-loader": "5.0.1",
    "cypress": "7.1.0",
    "cypress-waitfor": "1.1.0",
    "eslint": "7.32.0",
    "eslint-plugin-react": "7.24.0",
    "file-loader": "6.2.0",
    "mini-css-extract-plugin": "1.3.3",
    "node-sass": "5.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "path-browserify": "^1.0.1",
    "prettier": "2.3.2",
    "raw-loader": "4.0.2",
    "sass": "1.38.0",
    "sass-loader": "10.1.0",
    "speed-measure-webpack-plugin": "1.5.0",
    "style-loader": "2.0.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "3.11.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": false
        }
      ]
    ]
  }
}
